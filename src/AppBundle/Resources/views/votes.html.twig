{%- macro vote(entity) -%}
  {%- spaceless -%}
    <div class="vote">
      <a href="#" class="vote-up-link"><span class="inner">&uparrow;</span></a>
      <span class="vote-score" data-score="{{ entity.netScore }}">{{ entity.netScore }}</span>
      <a href="#" class="vote-down-link"><span class="inner">&downarrow;</span></a>
    </div>
  {%- endspaceless -%}
{% endmacro %}
