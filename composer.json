{
    "name": "postmill/postmill",
    "license": "Zlib",
    "type": "project",
    "description": "Free, web-based, social link aggregator with voting and nested comments.",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": "^7.1.3",
        "ext-apcu": "*",
        "ext-curl": "*",
        "ext-pdo_pgsql": "*",
        "derrabus/composer-parameter-handler": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "embed/embed": "^3.0",
        "ezyang/htmlpurifier": "^4.8",
        "friendsofsymfony/jsrouting-bundle": "^2.1",
        "gregwar/captcha": "dev-master#1d06ac8e12d2b3e0df39586d78b9f50e086a0bea as 1.1",
        "gregwar/captcha-bundle": "dev-master#639430383fd6aaedd421e39404445cd65d377f10 as 2.0",
        "league/commonmark": "^0.15.2",
        "liip/imagine-bundle": "^2.0-dev",
        "oneup/flysystem-bundle": "^3.0",
        "ramsey/uuid": "^3.7",
        "ramsey/uuid-doctrine": "^1.4",
        "sabberworm/php-css-parser": "^8.1",
        "sensio/framework-extra-bundle": "^5.0.1",
        "symfony/monolog-bundle": "^3.0",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-intl-grapheme": "^1.4",
        "symfony/polyfill-php71": "^1.3",
        "symfony/requirements-checker": "^1.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/symfony": "4.0.*",
        "twig/extensions": "^1.4",
        "twig/twig": "^2.0",
        "webuni/commonmark-table-extension": "^0.6.1",
        "white-october/pagerfanta-bundle": "^1.0",
        "willdurand/js-translation-bundle": "^2.7-dev"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.3",
        "symfony/phpunit-bridge": "^3.0"
    },
    "suggest": {
        "ext-intl": "Faster grapheme_* functions, localised timestamps"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "bin/console cache:clear",
            "bin/console assets:install --symlink --relative web",
            "cp vendor/symfony/requirements-checker/src/SymfonyRequirements.php var/",
            "cp vendor/symfony/requirements-checker/public/check.php web/check.php",
            "cp vendor/symfony/requirements-checker/bin/requirements-checker.php bin/symfony_requirements"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    }
}
